            );
         if new_rec.ctype_id is null then
            raise_application_error(-20020, 'Null ID was returned from card_types_tab.get_id');
         end if;
      end if;
   end if;
   -- Set new_rec.game_id, if needed
   if new_rec.game_id = old_rec.game_id then
